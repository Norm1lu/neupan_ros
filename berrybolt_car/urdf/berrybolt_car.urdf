  <!-- Gazebo differential drive plugin (top-level) to publish odom and TF and consume /cmd_vel -->
  <gazebo>
    <plugin name="gazebo_ros_diff_drive" filename="libgazebo_ros_diff_drive.so">
      <leftJoint>car_left_joint</leftJoint>
      <rightJoint>car_right_joint</rightJoint>
      <wheelSeparation>0.46</wheelSeparation>
      <wheelDiameter>1.0</wheelDiameter>
      <torque>100.0</torque>
      <commandTopic>/cmd_vel</commandTopic>
      <odometryTopic>/odom</odometryTopic>
      <odometryFrame>odom</odometryFrame>
      <robotBaseFrame>base_link</robotBaseFrame>
      <publishTf>true</publishTf>
      <publishOdomTF>true</publishOdomTF>
      <updateRate>100</updateRate>
    </plugin>
  </gazebo>

  <!-- Gazebo laser sensor on front_lidar_link to publish /limo/scan -->
